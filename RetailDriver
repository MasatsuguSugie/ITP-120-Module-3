package itp120mod3;

public class RetailDriver {

	public static void main(String[] args) {
		RetailItem ri = new RetailItem("Jacket",12,59.95);
		System.out.println(ri.ToString());
		RetailItem ri2 = new RetailItem();
		
		ri2.setDescription("Designer Jeans");
		ri2.setPrice(34.95);
		ri2.setUnitsOnHand(40);
		System.out.println(ri2.ToString());
		
		RetailItem ri3 = new RetailItem("Shirt", 20, 24.95);
		System.out.println(ri3.ToString());

	}

}
